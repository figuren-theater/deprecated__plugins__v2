{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "isc/image-source-control-isc-block",
	"version": "0.1.0",
	"title": "Image Sources",
	"category": "media",
	"description": "Show the Image-Sources managed via 'Image Source Control'.",
	"icon": "admin-media",
	"attributes": {
		"showAll": {
			"type": "boolean"
		}
	},
	"example": {
	    "attributes": {
	    	"showAll": true
	    }
	},
	"supports": {
		"color": {
			"text": true,
			"link": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalDefaultControls": {
				"fontSize": true,
				"fontAppearance": true,
				"textTransform": true
			}
		},
		"multiple": true,
		"html": false,
		"reusable": true
	},
	"textdomain": "image-source-control-isc-block",
	"editorScript": "file:index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
